{
    "name": "frello",
    "version": "1.0.0",
    "description": "A Trello clone",
    "scripts": {
        "start": "nodemon -w src -e js,ts,json --exec \"ts-node src\"",
        "build": "tsc",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint --fix src/**/*.ts",
        "format": "prettier --write src/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jhonnold/frello.git"
    },
    "author": "Jay Honnold <jayhonnold@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jhonnold/frello/issues"
    },
    "homepage": "https://github.com/jhonnold/frello#readme",
    "dependencies": {
        "bcrypt": "^4.0.1",
        "body-parser": "^1.19.0",
        "chalk": "^4.0.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.5.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "mongoose": "^5.9.16",
        "morgan": "^1.10.0",
        "on-finished": "^2.3.0",
        "uuid": "^8.0.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.6",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.150",
        "@types/mongoose": "^5.7.21",
        "@types/node": "^13.13.0",
        "@types/on-finished": "^2.3.1",
        "@types/uuid": "^7.0.3",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-loader": "^4.0.0",
        "eslint-plugin-prettier": "^3.1.3",
        "nodemon": "^2.0.2",
        "prettier": "^2.0.2",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    }
